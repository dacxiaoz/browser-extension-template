{
    "name": "浏览器插件扩展模版",
    "version": "1.0.0",
    "description": "浏览器插件扩展模版",
    "icons": {
      "16": "icons/icon-16x16.png",
      "48": "icons/icon-48x48.png",
      "128": "icons/icon-128x128.png"
    },
    "automation": true,
    "manifest_version": 3,
    "permissions": ["contextMenus", "<all_urls>", "storage", "tabs"],
    "host_permissions": ["<all_urls>"],
      "background": {
          "service_worker": "extension/background/index.js"
      },
    "web_accessible_resources": [
        {
            "resources": ["extension/page-script/index.js"],
            "matches": ["<all_urls>"]
        }
    ],
    "content_scripts": [
        {
            "js": ["/extension/content-script/index.js"],
            "run_at": "document_start",
            "matches": ["<all_urls>"]
        }
    ],
    "action": {
      "default_title": "浏览器插件扩展模版",
      "default_popup": "popup.html"
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
    },
    "sandbox": {
      "pages": ["https://*.zhiyitech.cn"]
    }
}